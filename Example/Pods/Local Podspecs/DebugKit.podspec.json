    "tag": "0.1.0"
  },
  "default_subspecs": "Core",
  "platforms": {
    "ios": "10.0"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "DebugKit/Core/**/*.{swift}",
      "resource_bundles": {
        "Core": [
          "DebugKit/Core/**/*.xib"
        ]
      }
    },
    {
      "name": "Log",
      "source_files": "DebugKit/Log/**/*.{swift}",
      "resource_bundles": {
        "Log": [
          "DebugKit/Log/**/*.xib"
        ]
      }
    },
    {
      "name": "JsonViewer",
      "source_files": "DebugKit/JsonViewer/**/*.{swift}",
      "resource_bundles": {
        "JsonViewer": [
          "DebugKit/JsonViewer/**/*.{html,js,css}"
        ]
      }
    },
    {
      "name": "UserDefaults",
      "source_files": "DebugKit/UserDefaults/**/*.{swift}",
      "resource_bundles": {
        "UserDefaults": [
          "DebugKit/UserDefaults/**/*.{xib}"
        ]
      }
    }
  ]
}
